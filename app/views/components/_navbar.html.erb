<div class="navbar">
  <div class="logo">rent a techy</div>
  <div class="search"> <%= user.present? %></div>
  <nav>
    <% unless user.present? %>
      <%= link_to 'Sign Up', new_user_registration_path, class: "button button-primary" %>
      <%= link_to 'Sign In', new_user_session_path, class: "button button-text" %>
    <% else %>
      <%= link_to user[:user_name], '/', class: "button button-text" %>
      <%= link_to 'Log out', destroy_user_session_path,method:'delete', class: "button button-secondary" %>
    <% end %>
  </nav>



</div>